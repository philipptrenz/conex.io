{
	"information": {
		"filename": "fs20.json",
		"description": "Mapping instructions for the FHEM module 'FS20'",
		"author": "Philipp Trenz, Benjamin Brausse",
		"last_edited": "2017-04-10"
	},
	"device_id": {
		"key_path": "Name"
	},
	"type_id": {
		"key_path": "Internals/TYPE"
	},
	"rooms": {
		"key_path": "Attributes/room",
		"delimiters": ","
	},
	"groups": {
		"key_path": "Attributes/group",
		"delimiters": ","
	},
	"functions": {
		"get": [
			{
				"function_id": "onoff",
				"class_name": "io.swagger.model.OnOff",
				"requirements": [
					{
						"mode": "contains_all",
						"key_path": "PossibleSets",
						"attributes": [
							"on",
							"off"
						],
						"delimiters": " "
					}
				],
				"properties": {
					"isOn": {
						"type": "boolean",
						"default": false,
						"key_path": "Readings/state/Value",
						"cases": [
							{
								"extract_mode": "direct",
								"value": true,
								"regex": [
									"on",
									"dim([0-9]{1,2}|100)%"
								]
							},
							{
								"extract_mode": "direct",
								"value": false,
								"regex": [
									"off"
								]
							}
						]
					},
					"timestamp": {
						"type": "date-time",
						"key_path": "Readings/state/Time",
						"format": "yyyy-MM-dd HH:mm:ss"
					}
				}
			},
			{
				"function_id": "dimmer",
				"class_name": "io.swagger.model.Dimmer",
				"requirements": [
					{
						"mode": "contains_all",
						"key_path": "PossibleSets",
						"attributes": [
							"on",
							"off",
							"dim([0-9]{1,2}|100)%"
						],
						"delimiters": " "
					}
				],
				"properties": {
					"value": {
						"type": "integer",
						"default": 0,
						"key_path": "Readings/state/Value",
						"cases": [
							{
								"extract_mode": "range",
								"minimum": 0,
								"maximum": 100,
								"regex": "dim([0-9]{1,2}|100)%"
							},
							{
								"extract_mode": "direct",
								"constraint": "min",
								"regex": [
									"off"
								]
							},
							{
								"extract_mode": "direct",
								"constraint": "max",
								"regex": [
									"on"
								]
							}
						]
					},
					"timestamp": {
						"type": "date-time",
						"key_path": "Readings/state/Time",
						"format": "yyyy-MM-dd HH:mm:ss"
					}
				}
			}
		],
		"set": [
			{
				"function_id": "onoff",
				"class_name": "io.swagger.model.OnOff",
				"properties": {
					"isOn": {
						"type": "boolean",
						"cases": [
							{
								"inject_mode": "direct",
								"value": true,
								"parameter": "on"
							},
							{
								"inject_mode": "direct",
								"value": false,
								"parameter": "off"
							}
						]
					}
				}
			},
			{
				"function_id": "dimmer",
				"class_name": "io.swagger.model.Dimmer",
				"properties": {
					"isOn": {
						"type": "integer",
						"cases": [
							{
								"inject_mode": "from_to",
								"minimum": 0,
								"maximum": 100,
								"mapping": [
									{
										"from": 0,
										"to": 0,
										"parameter": "off"
									},
									{
										"from": 1,
										"to": 6,
