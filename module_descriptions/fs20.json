	"read": {
		"onoff": {
			"classname":"io.swagger.model.OnOff",
			"requirements": [
				{
					"mode":"contains_all",
					"key_path":"PossibleSets",
					"attributes": [ "on", "off" ],
					"delimiters": " "
				}
			],
			"properties": {
                "isOn": {
                    "type": "boolean",
                    "default": false,
                    "key_path":"Readings/state/Value",
                    "extract_mode":"bool",
                    "cases": [ 
                    	{
                    		"value": true,
                    		"regex": [ "on", "dim[0-9]{1,3}%" ]
                    	}, 
                    	{
                    		"value": false,
                    		"regex": [ "off" ]
                    	}
                    ]
                }
            }
		},
		"slider": {
			"classname":"io.swagger.model.Slider"
		
		
			
		
		}
	},
